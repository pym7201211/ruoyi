    <delete id="deleteTokenApplyFormById" parameterType="Long">
        delete from token_apply_form where token_id = #{tokenId}
    </delete>

    <delete id="deleteTokenApplyFormByIds" parameterType="String">
        delete from token_apply_form where token_id in
        <foreach item="tokenId" collection="array" open="(" separator="," close=")">
            #{tokenId}
        </foreach>
    </delete>

    <select id="selectTokenAutoDelayList" parameterType="String" resultMap="TokenApplyFormResult">
        select * from token_apply_form a where a.reserved2 is null and a.seqno not in (
        select seq_no from urgency_task
        ) and a.approval_status = '1' and a.end_date between sysdate and  sysdate+0.25/24
    </select>

    <update id="updateTokenAutoDelay" parameterType="String">
        update token_apply_form a set a.reserved2 = '1' where a.seqno = #{seqno}
    </update>

